<Window x:Name="mainWindow" x:Class="Tema3_Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3_Mosaico"
        mc:Ignorable="d"
        Title="Mosaico Colores" Height="695" Width="665" Icon="/assets/mosaic.ico">
    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="BlueViolet"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="BlueViolet"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="ForestGreen"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="ForestGreen"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="Red"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Red"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="Orange"></TextBlock>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="Orange"></TextBlock>
        <TextBlock Background="Black"></TextBlock>
    </DockPanel>
</Window>
